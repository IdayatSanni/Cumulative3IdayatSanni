@model SchoolProject.Models.Class
@{
    ViewBag.Title = "Show";
}

<a class="navi-link" href="/Class/List">Back to Classes</a>
<div id="containershow">
    <h2>Class Name is :@Model.ClassCode @Model.ClassName </h2>
    <div>
        <h4>
            Start Date: <em>@Model.StartDate.ToString("dd-MMM-yyyy")</em>
        </h4>
    </div>
    <div>
        <h4> Finish Date: <em>@Model.FinishDate.ToString("dd-MMM-yyyy")</em> </h4>
    </div>
    
</div>

