@model SchoolProject.Models.Teacher
@{
    ViewBag.Title = "Show";
}

<a class="navi-link" href="/Teacher/List" >Back to Teachers</a>
<a class="navi-link" href="/Teacher/Update/@Model.TeacherId">Update Teacher</a>
<a class="del-link" href="/Teacher/DeleteConfirm/@Model.TeacherId">Delete</a>

<div id="containershow">
    <h2>@Model.TeacherFname @Model.TeacherLname </h2>
    <div>
        <h4>
            Salary: @Model.Salary
        </h4>
    </div>
    <div>
        <h4 style="text-align:center">Hire Date: @Model.HireDate.ToString("dd-MMM-yyyy")</h4>
    </div>
</div>


