@model SchoolProject.Models.Student
@{
    ViewBag.Title = "Show";
}

<a class="navi-link" href="/Student/List">Back to Students</a>
<div id="containershow">
    <h2>@Model.StudentFname @Model.StudentLname </h2>
    <div>
        <h4>
           Student Number: @Model.StudentNumber
        </h4>
    </div>
    <div>
        <p style="text-align:center">Enrolment Date: @Model.EnrolDate.ToString("dd-MMM-yyyy")</p>
    </div>
</div>

